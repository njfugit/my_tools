{
  "name": "natsort",
  "version": "2.0.2",
  "description": "Javascript natural sort algorithm with unicode support.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "dist/*.js"
  ],
  "directories": {
    "lib": "lib/",
    "dist": "dist/"
  },
  "keywords": [
    "natural",
    "natsort",
    "sort",
    "sorter",
    "numeric",
    "unicode",
    "algorithm",
    "date",
    "datetime",
    "desc",
    "case-insensitive"
  ],
  "author": {
    "name": "bubkooo",
    "email": "bubkoo.wy@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netop/natsort.git"
  },
  "bugs": {
    "url": "https://github.com/netop/natsort/issues"
  },
  "homepage": "https://github.com/netop/natsort#readme",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "coveralls": "^3.0.2",
    "jest": "^23.5.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.0.3",
    "uglify-js": "^2.6.2"
  },
  "scripts": {
    "lint": "tslint ./src/*.ts -c tslint.json -p tsconfig.json --fix",
    "clean": "rimraf lib && rimraf dist",
    "pretest": "rimraf ./test/coverage",
    "test": "jest",
    "coveralls": "cat ./test/coverage/lcov.info | coveralls",
    "prepublish": "npm test",
    "uglify": "uglifyjs ./dist/index.js -o ./dist/natsort.min.js -m -c",
    "build:lib": "tsc",
    "build:dist": "tsc -d false -m UMD --outDir ./dist && npm run uglify && rimraf ./dist/index.js",
    "prebuild": "npm run clean",
    "build": "npm run build:lib && npm run build:dist"
  }
}
